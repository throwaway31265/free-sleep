{
  "name": "server",
  "type": "module",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "dotenv -e .env.pod -- bun run src/server.ts",
    "build": "tsc",
    "dev": "dotenv -e .env.pod -- bun --watch src/server.ts",
    "dev:local": "dotenv -e .env.local -- bun --watch src/server.ts",
    "lint": "bun x eslint src/ --ext .ts,.tsx",
    "migrate": "dotenv -e .env.pod -- bun x prisma migrate dev --name ",
    "generate": "dotenv -e .env.pod -- bun x prisma generate",
    "migrate:local": "dotenv -e .env.local -- bun x prisma migrate dev --name ",
    "generate:local": "dotenv -e .env.local -- bun x prisma generate"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^6.4.1",
    "bree": "^9.2.4",
    "cbor": "^9.0.2",
    "chokidar": "^4.0.3",
    "cors": "^2.8.5",
    "dotenv-cli": "^8.0.0",
    "express": "^4.21.2",
    "lodash": "^4.17.21",
    "lowdb": "^6.1.1",
    "moment-timezone": "^0.5.46",
    "node-schedule": "^2.1.1",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^3.0.0"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^22.13.5",
    "@types/node-schedule": "^2.1.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.57.1",
    "nodemon": "^3.1.9",
    "prisma": "^6.4.1",
    "ts-essentials": "^10.0.4",
    "ts-node": "^10.9.2",
    "typescript": "5.7.3"
  }
}
